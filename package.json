{
  "name": "jsdoc-md",
  "version": "0.0.0",
  "private": true,
  "description": "A CLI to analyze source JSDoc and generate documentation under a given heading in a markdown file (such as readme.md).",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/jsdoc-md",
  "homepage": "https://github.com/jaydenseric/jsdoc-md#readme",
  "bugs": "https://github.com/jaydenseric/jsdoc-md/issues",
  "keywords": [
    "jsdoc",
    "md",
    "markdown",
    "readme",
    "babel",
    "remark",
    "unified"
  ],
  "files": [
    "lib",
    "index.js",
    "cli.js",
    "!*.test.js"
  ],
  "bin": "cli.js",
  "engines": {
    "node": ">=8.5"
  },
  "dependencies": {
    "@babel/parser": "^7.0.0-beta.51",
    "doctrine": "^2.1.0",
    "github-slugger": "^1.2.0",
    "globby": "^8.0.1",
    "mdast-util-inject": "^1.1.0",
    "remark-parse": "^5.0.0",
    "remark-stringify": "^5.0.0",
    "remark-toc": "^5.0.0",
    "unified": "^7.0.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-env": "^0.5.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.13.7",
    "tap": "^12.0.1"
  },
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:jsdoc": "node cli",
    "prepare:prettier": "prettier readme.md --write",
    "test": "FORCE_COLOR=1 TAP_COLORS=1 run-p test:* -c --aggregate-output",
    "test:eslint": "eslint .",
    "test:prettier": "prettier '**/*.{json,md}' -l",
    "test:js": "tap ./lib/*.test.js -R spec",
    "prepublishOnly": "run-s prepare test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{json,md}": "prettier -l"
  },
  "eslintIgnore": [
    "tap-snapshots"
  ],
  "eslintConfig": {
    "extends": [
      "env"
    ],
    "overrides": [
      {
        "files": "*.test.js",
        "rules": {
          "node/no-unpublished-require": "off"
        }
      }
    ]
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  }
}
